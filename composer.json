{
    "name": "flashtag/services",
    "description": "Flashtag services...",
    "keywords": ["webhooks", "commands", "schedule"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-update-cmd": "cd .git-subsplit; sh install.sh",
        "post-install-cmd": "cd .git-subsplit; sh install.sh",
        "post-create-project-cmd": "cd .git-subsplit; sh install.sh"
    }
}
